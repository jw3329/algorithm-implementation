   "source": [
    "def LIS(arr):\n",
    "    n = len(arr)\n",
    "    dp = [1 for _ in range(n)]\n",
    "    for i in range(1,n):\n",
    "        for j in range(i):\n",
    "            if arr[i] > arr[j]:\n",
    "                dp[i] = max(dp[i],dp[j]+1)\n",
    "    return max(dp)"
   ]
  },
